local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("2z Hub", "Ocean")

--MAIN
local ScriptHub = Window:NewTab("Scripts")
local ScriptSection = ScriptHub:NewSection("Scripts")


ScriptSection:NewButton("Combat Warriors Script)", "Combat Warriors Script", function()
    loadstring(game:HttpGet("https://projecthook.xyz/scripts/new-free.lua"))()"))()
end)


